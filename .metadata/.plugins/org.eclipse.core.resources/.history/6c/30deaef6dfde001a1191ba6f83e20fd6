#ifndef TASKS_BMX055_H_
#define TASKS_BMX055_H_

/**
	SPI 4 wire interface
	Maximum frequency (VDDIO>1.75): 	10 MHz
	Automatic selection (CPOL,CPHA): 	default (0,0) & (1,1)
	CSB status: 						idle 1 & active 0
**/



#endif /* TASKS_BMX055_H_ */
